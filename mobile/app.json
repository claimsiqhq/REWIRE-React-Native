{
  "expo": {
    "name": "REWIRE",
    "slug": "rewire",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "rewire",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#1E1E2E"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.rewire.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#1E1E2E"
      },
      "package": "com.rewire.app"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-secure-store",
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow REWIRE to access your photos for your vision board."
        }
      ],
      [
        "expo-av",
        {
          "microphonePermission": "Allow REWIRE to access your microphone for voice features."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
